<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Example bean definition -->
    <bean id="messageService" class="com.revature.spring_io.MessageService"
     init-method="init">
    
     <constructor-arg value="Hello from spring Configuration!"/>
    </bean>

    <bean id="messageService2" class="com.revature.spring_io.MessageService">
         <property name="message" value="Hello from method!"/>
    </bean>

</beans>