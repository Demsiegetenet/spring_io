<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Example bean definition -->
    <bean id="app" class="com.revature.spring_io.App"
    init-method="init" destroy-method="cleanup">
     <constructor-arg value="Hello from constructor!"/>
    </bean>

    <bean id="app1" class="com.revature.spring_io.App">
     <property name="message" value="Hello from method!"/>
    </bean>

</beans>